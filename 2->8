#include <stdio.h>
#include <stdlib.h>   

int main()
{
    const char* binNumbers[] = {
        "10001110111", "101111", "11110000", "111110000",
        "111000", "11111000", "11011", "110111", "01110", "111111"
    };

    int count = sizeof(binNumbers) / sizeof(binNumbers[0]);

    printf("Преобразуване на двоични числа в осмична бройна система:\n\n");

    for (int i = 0; i < count; i++)
    {
        long decimal = strtol(binNumbers[i], NULL, 2);

        printf("Bin: %s => Oct: %lo\n", binNumbers[i], decimal);
    }

    return 0;
}
